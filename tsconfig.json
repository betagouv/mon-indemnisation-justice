{
  "compilerOptions": {
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "NodeNext",
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": ["vite/client", "vitest/globals"],
    "allowImportingTsExtensions": true,
     "baseUrl": ".",
      "paths": {
        "@/*": ["./assets/*"]
      }
  }
}