{% extends 'base_react.html.twig' %}

{% block title %}{{'homepage.suivi_dossier'|trans}}{% endblock %}


{% block body %}
  {% set csrf_token = csrf_token('authenticate') %}
  {% set user = app.user is not null ? app.user : empty_user() %}
  <div {{ stimulus_controller('homepage_suivi_dossier',{
    user: user|serialize('json',{groups: 'user:read'}),
    breadcrumb: breadcrumb|serialize('json'),
    version: version|serialize('json'),
    raccourci: raccourci,
    statuts: statuts,
    csrfToken: csrf_token
  }) }}></div>
{% endblock %}
