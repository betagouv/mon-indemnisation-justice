{% extends 'base_react.html.twig' %}

{% block title %}{{'cgu.title'|trans}}{% endblock %}


{% block body %}
  {% set user = app.user is not null ? app.user : empty_user() %}
  <div {{ stimulus_controller('homepage_cgu',{
    user: user|serialize('json',{groups: 'user:read'}),
    breadcrumb: breadcrumb|serialize('json'),
    version: version|serialize('json')
  }) }}></div>
{% endblock %}
