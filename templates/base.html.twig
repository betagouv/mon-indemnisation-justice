<!DOCTYPE html>
<html
  lang="{{ app.request.locale|split('_')[0] }}"
>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <!-- ZONE DSFR -->
        <link rel="apple-touch-icon" href="/dsfr/favicon/apple-touch-icon.png" />
        <link rel="icon" href="/dsfr/favicon/favicon.svg" type="image/svg+xml" />
        <link rel="shortcut icon" href="/dsfr/favicon/favicon.ico" type="image/x-icon" />
        <link rel="manifest" href="/dsfr/favicon/manifest.webmanifest" crossorigin="use-credentials" />
        <link rel="stylesheet" href="/dsfr/utility/icons/icons.min.css" />
        <link rel="stylesheet" href="{{ encore_entry_link_tags('dsfr') }}" />
        <!-- /ZONE DSFR -->
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
      <div class="fr-container--fluid">
          <div class="fr-grid-row">
              <div class="fr-col-12">
              {% include('header.html.twig') %}
              </div>
              <div class="fr-col-12">{{ include('breadcrumb.html.twig') }}</div>
              <div class="fr-col-12">
              {% block body %}{% endblock %}
              </div>
          </div>
      </div>
    </body>
</html>
